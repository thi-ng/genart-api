{
	"name": "@thi.ng/genart-api",
	"version": "0.0.1",
	"description": "Platform-independent extensible API for browser-based generative art",
	"repository": "https://github.com/thi-ng/genart-api",
	"author": "Karsten Schmidt <k+npm@thi.ng>",
	"license": "Apache-2.0",
	"type": "module",
	"private": "true",
	"scripts": {
		"build": "node build.js && yarn build:types",
		"build:examples": "node build-examples.js",
		"build:types": "tsc -d --emitDeclarationOnly -t esnext -m esnext -outDir dist src/api.ts",
		"doc": "typedoc --excludePrivate --excludeInternal --out doc src/api.ts"
	},
	"devDependencies": {
		"@types/node": "^22.7.5",
		"typedoc": "^0.26.7",
		"typescript": "^5.6.2",
		"esbuild": "^0.24.0"
	},
	"sideEffects": false,
	"dependencies": {}
}
