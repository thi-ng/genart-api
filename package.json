{
	"name": "@thi.ng/genart-api",
	"version": "0.0.1",
	"description": "TODO",
	"repository": "https://github.com/thi-ng/genart-api",
	"author": "Karsten Schmidt <k+npm@thi.ng>",
	"license": "Apache-2.0",
	"type": "module",
	"scripts": {
		"start": "vite --host --open",
		"build": "tsc && vite build --base='./' && cp *.min.js dist",
		"build:api": "yarn build:lib && yarn build:adapter",
		"build:lib": "yarn esbuild --bundle --platform=browser --minify --target=es2022 --outfile=lib.min.js src/index.ts",
		"build:adapter": "yarn esbuild --bundle --platform=browser --minify --target=es2022 --outfile=adapter.min.js src/adapters/urlparams.ts",
		"preview": "vite preview --host --open",
		"doc": "typedoc --excludePrivate --excludeInternal --out doc src/api.ts"
	},
	"devDependencies": {
		"typedoc": "0.26.7",
		"typescript": "^5.6.2",
		"vite": "^5.4.5",
		"vite-plugin-html": "^3.2.2"
	},
	"sideEffects": false,
	"dependencies": {
		"@thi.ng/canvas": "^0.5.2",
		"@thi.ng/geom": "^8.1.8",
		"@thi.ng/hiccup-canvas": "^3.1.17",
		"@thi.ng/imgui": "^2.3.2",
		"@thi.ng/layout": "^3.1.1",
		"@thi.ng/ramp": "^3.3.1",
		"@thi.ng/rdom": "^1.7.2",
		"@thi.ng/rdom-forms": "^0.3.30",
		"@thi.ng/rstream": "^9.0.3",
		"@thi.ng/rstream-gestures": "^5.0.91",
		"@thi.ng/vectors": "^7.12.1"
	}
}
